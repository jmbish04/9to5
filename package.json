{
  "name": "9to5",
  "description": "Job board and recruitment SaaS platform built with Cloudflare Workers, D1, and Astro.",
  "private": true,
  "cloudflare": {
    "label": "SaaS Job Board Platform",
    "summary": "A job board and recruitment SaaS platform built with Cloudflare Workers",
    "products": [
      "Workers",
      "D1"
    ],
    "categories": [],
    "icon_urls": [
      "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/c6fc5da3-1e0a-4608-b2f1-9628577ec800/public",
      "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/5ca0ca32-e897-4699-d4c1-6b680512f000/public"
    ],
    "docs_url": "https://developers.cloudflare.com/workers/",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/0f9ff766-eb55-4eee-1ad6-efeb981fe800/public",
    "publish": true
  },
  "dependencies": {
    "@astrojs/cloudflare": "12.6.0",
    "@astrojs/react": "4.3.0",
    "@astrojs/tailwind": "6.0.2",
    "@hookform/resolvers": "5.1.1",
    "@radix-ui/react-accordion": "1.2.7",
    "@radix-ui/react-dialog": "1.1.10",
    "@radix-ui/react-label": "2.1.4",
    "@radix-ui/react-navigation-menu": "1.2.9",
    "@radix-ui/react-slot": "1.2.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@tanstack/react-table": "8.21.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "astro": "5.10.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "0.525.0",
    "qs": "^6.12.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.58.1",
    "swr": "^2.2.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.25.67",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/qs": "^6.9.11",
    "openapi-typescript": "^6.7.0",
    "wrangler": "4.37.0",
    "puppeteer": "^23.0.0"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro build && cp src/workflows/wrapper.js dist/index.js",
    "cf-typegen": "wrangler types",
    "check": "pnpm run build && pnpm run wrangler:wrapper && wrangler deploy --dry-run",
    "db:migrate": "mkdir -p .d1 && cat migrations/*.sql | sqlite3 .d1/DB.sqlite",
    "db:migrate:remote": "wrangler d1 migrations apply DB --remote",
    "deploy": "pnpm run build && pnpm run wrangler:wrapper && wrangler deploy",
    "dev": "pnpm run db:migrate && pnpm run build && pnpm run wrangler:wrapper && wrangler dev --persist-to ./.d1 --port 4321",
    "predeploy": "pnpm run db:migrate:remote",
    "preview": "pnpm run build && pnpm run wrangler:wrapper && wrangler dev",
    "wrangler:wrapper": "cp src/workflows/wrapper.js dist/index.js",
    "generate-types": "wrangler types",
    "types:api": "openapi-typescript public/openapi.json -o src/lib/types/openapi.ts",
    "typecheck": "pnpm run generate-types && tsc -p tsconfig.json",
    "migrate:local": "wrangler d1 migrations apply JOB_SCRAPER_DB --local",
    "migrate:remote": "wrangler d1 migrations apply JOB_SCRAPER_DB --remote",
    "dev:smoke": "node scripts/dev-smoke.mjs",
    "test": "node scripts/e2e-test.mjs",
    "test:smoke": "node scripts/dev-smoke.mjs",
    "test:e2e": "node scripts/e2e-test.mjs",
    "test:ai": "node scripts/test-ai-agents.mjs"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  },
  "engines": {
    "node": "22.x"
  },
  "packageManager": "pnpm@9.0.0"
}